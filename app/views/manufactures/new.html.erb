<div class="form">
    <%= form_for @manufacture, :html => {:multipart => true} do |f| %>

        <p>
            <%= f.label :name %>
            <%= f.text_field :name %>
        </p>

        <p>
            <%= f.label :description%>
            <%= f.text_area :description %><br>
        </p>
        <p>
            <%= f.label :link%>
            <%= f.text_field :link %><br>
        </p>

        <p>
            <%=f.file_field :image%>
        </p>


        <p><%= f.submit "Submit" %> </p>
    <% end %>
</div>
<div class="new_manufacture">

    <input type="button" onclick="history.back();" value="Назад"/>

</div>